{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "createwithrani/superlist-block",
	"version": "0.1.0",
	"title": "Super List",
	"category": "design",
	"description": "Nest multiple blocks inside list items in any kind of list (ordered, unordered, no marker, etc)",
	"attributes": {
		"listStyle":{
			"type": "string",
			"default": "ul"
		},
		"orientation": {
			"type": "string",
			"default": "vertical"
		},
		"itemWidth": {
			"type": "string",
			"default": "350px"
		}
	},
	"supports": {
		"html": true,
		"align": true,
		"color": {
			"background": true,
			"gradients": true,
			"text": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "superlist-block",
	"editorScript": "file:../../build/superlist.js",
	"editorStyle": "file:../../build/superlist.css",
	"style": "file:../../build/style-superlist.css"
}
